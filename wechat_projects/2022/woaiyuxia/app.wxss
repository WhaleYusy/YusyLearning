/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.container2{
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.container3{
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  padding: 40rpx 0;
}
.title{
  font-size: 34rpx;
}
.img1{
  width: 640rpx;
  height: calc(640 / 1280 * 720rpx);
}
.img2{
  width: 480rpx;
  height: calc(480 / 800 * 1422rpx);
}
.img3{
  width: 480rpx;
  height: calc(480 / 600 * 800rpx);
}
.img4{
  width: 480rpx;
  height: calc(480 / 960 * 1280rpx);
}
.img5{
  width: 640rpx;
  height: calc(640 / 1280 * 2276rpx);
}
.img6{
  width: 640rpx;
  height: calc(640 / 1280 * 960rpx);
}
.img1,.img2,.img3,.img4,.img5{
  margin: 20rpx;
}
.title2,.title3{
  font-size: 28rpx;
  text-align: center;
}
.title3{
  color: red;
}

@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1631015_njv9e3ga16d.eot?t=1581128894523'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1631015_njv9e3ga16d.eot?t=1581128894523#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1631015_njv9e3ga16d.woff?t=1581128894523') format('woff'),
  url('//at.alicdn.com/t/font_1631015_njv9e3ga16d.ttf?t=1581128894523') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1631015_njv9e3ga16d.svg?t=1581128894523#iconfont') format('svg'); /* iOS 4.1- */
