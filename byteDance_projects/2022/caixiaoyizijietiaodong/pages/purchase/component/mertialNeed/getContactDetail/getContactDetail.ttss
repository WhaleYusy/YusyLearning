/* pages/purchase/component/mertialNeed/getContactDetail/getContactDetail.wxss */
.getContactDetailContent{
  margin-bottom:120rpx;
}
.getContactDetail {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: #f8f8f8;
  height: calc(100vh - 120rpx);
  overflow: auto;
}
.getContactDetail .reward_cont {
  background: white;
  width: 95%;
  margin: 20rpx auto;
  border-radius: 18rpx;
  padding: 25rpx 0;
  box-sizing: border-box;
}
.getContactDetail .reward_cont .reward {
  font-size: 30rpx;
  text-align: center;
  line-height: 60rpx;
  border: 1rpx solid transparent;
}
.getContactDetail .reward_cont .reward .font-size {
  font-weight: bold;
  font-size: 35rpx;
}
.getContactDetail .reward_cont .reward .line-through {
  text-decoration: line-through;
}
.getContactDetail .reward_cont .reward .line-pay {
  color: red;
  padding-left: 5rpx;
}
.getContactDetail .reward_cont .reward .red {
  color: red;
  text-decoration: underline;
  font-weight: bold;
}
.getContactDetail .reward_cont .size {
  font-size: 24rpx;
}
.getContactDetail .reward_cont .tips {
  width: 90%;
  margin: 0rpx auto;
  font-size: 28rpx;
  line-height: 70rpx;
}
.getContactDetail .reward_cont .tip_title {
  color: #3e5af8;
  text-align: center;
  font-size: 24rpx !important;
}
.getContactDetail .reward_cont .tip_title .mini_tips {
  text-align: center;
}
.getContactDetail .reward_cont .tip_title .biCount-pay {
  text-decoration: line-through;
}
.getContactDetail .reward_cont .tip_title .biCount-pays {
  color: #3e5af8;
  padding-left: 5rpx;
}
.getContactDetail .reward_cont .give_line {
  text-align: center;
  width:100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  font-size: 30rpx;
  background: #030CF9;
  border-radius: 40rpx;
 cursor:pointer;
 letter-spacing: 4rpx;
  /* padding:0; */
}
.contactButtons{
  width: 90%;
  margin: 0 auto; 
  position: relative;
}
.contactButtons .btn_1{
  width:110rpx;
  height:110rpx;
