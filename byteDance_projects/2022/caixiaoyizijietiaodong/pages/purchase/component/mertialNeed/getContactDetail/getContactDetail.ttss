  display: flex;
  align-items: centent;
  justify-content: center;
}
.getContactDetail .reward_cont .my_money text {
  width: 200rpx !important;
  height: 40rpx !important;
  line-height: 40rpx;
  border: 2rpx solid #f40603;
  color: #f40603;
  background: white;
  margin-left: 10rpx;
  outline: none;
  font-size: 25rpx;
  border-radius: 40rpx;
}
.getContactDetail .reward_cont .my_follow {
  width: 90%;
  margin: 10rpx auto;
  line-height: 50rpx;
  font-size: 25rpx;
}
