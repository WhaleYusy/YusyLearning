  .footer-warp {
    padding: 20rpx;
  }
  .footer-warp .footer-msgs {
    background: white;
    font-size: 28rpx;
    color: #666666;
    padding: 30rpx 20rpx;
    border-radius: 10rpx;
  }
  .footer-warp .footer-bottom {
    background: white;
    border-radius: 10rpx;
    padding: 50rpx 0;
  }
  .footer-warp .footer-bottom .statistics {
    display: flex;
    justify-content: space-around;
    margin-bottom: 50rpx;
  }
  .footer-warp .footer-bottom .statistics .flex {
    line-height: 50rpx;
    text-align: center;
    font-size: 28rpx;
  }
  .footer-warp .footer-bottom .statistics .flex text {
    font-size: 24rpx;
    color: #666;
  }
  .footer-warp .footer-bottom .ad-footer {
    font-size: 28rpx;
    text-align: center;
  }
  

