.layer{
  z-index:4000;
  position:fixed;
  top:0;
  right:0;
  bottom:0;
  left:0;
  background:rgba(0,0,0,0.6);
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.layer button{
  width:400rpx;
  margin-top:30rpx;
  background:#3A8BEE;
  color:white;
}
