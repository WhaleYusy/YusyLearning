    display: flex;
    padding-top: 176rpx;
    position: relative;
  }
   .coupon-box .bottom .no-address .no-box {
    box-sizing: border-box;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    justify-content: center;
    flex-direction: column;
  }
   .coupon-box .bottom .no-address .no-box image {
    margin: auto;
    width: 207rpx;
    height: 241rpx;
  }
   .coupon-box .bottom .no-address .no-box p {
    text-align: center;
    color: #aaaaaa;
    font-size: 24rpx;
    padding-top: 30rpx;
  }
  
