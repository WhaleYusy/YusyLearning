.header{
  display:flex;
  align-items: center;
  position:fixed;
  top:0;
  height: 75rpx;
  font-size: 24rpx;
  z-index: 1000;
  width: 100%;
  background: #fff;
  border-top: 2rpx solid #eee;
}
.toAdminIcon{
  padding: 20rpx;
}
.toAdminCenter{
  flex:1;
  padding:20rpx 10rpx;
  text-align: center;
  font-size: 32rpx;
}
.fillOrderContent{
  margin-top: 75rpx;
  margin-bottom:120rpx;
}
   .top {
    height: 70rpx;
    box-sizing: border-box;
  }
  .coupon-box .bottom{
    height:60vh;
  }
   .bottom {
    height: calc(100% - 168rpx);
    box-sizing: border-box;
    overflow: auto;
  }
   .bottom .address-box {
     text-align: center;
    height: 180rpx;
    width: 100%;
    box-sizing: border-box;
    margin-top: 20rpx;
    background: white;
    display: flex;
  }
   .bottom .address-box .no-sel-address {
    margin: auto;
    font-size: 24rpx;
    color: #292621;
  }
   .bottom .address-box .address-info {
    box-sizing: border-box;
    display: flex;
    width: 100%;
    justify-content: center;
    align-items: center;

  }
   .bottom .address-box .address-info .info-left {
    margin: auto;
    width: 300rpx;
    padding: 0 28rpx;
    flex: 1;
  }
   .bottom .address-box .address-info .info-left .title {
    color: #1a1a1a;
    font-size: 32rpx;
  }
   .bottom .address-box .address-info .info-left .title text {
    margin-left: 28rpx;
  }
   .bottom .address-box .address-info .info-left .info {
    font-size: 24rpx;
    line-height: 36rpx;
    color: #292621;
    margin-top: 20rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    padding: 3rpx 0;
  }
   .bottom .address-box .address-info .info-right {
    width: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
  }
   .bottom .address-box .address-info .info-right i {
    font-size: 32rpx;
    color: #666666;
  }
   .bottom .goods-warp {
    margin-top: 20rpx;
  }
   .bottom .goods-warp .goods-box {
    width: 100%;
    height: 320rpx;
    box-sizing: border-box;
    background: white;
    display: flex;
    flex-direction: column;
    padding: 28rpx;
    border-bottom: 2rpx solid #f7f7f7;
  }
   .bottom .goods-warp .goods-box .top {
    flex: 1;
    box-sizing: border-box;
    display: flex;
  }
   .bottom .goods-warp .goods-box .top .top-info {
    margin: auto;
    box-sizing: border-box;
    display: flex;
    width: 100%;
  }
   .bottom .goods-warp .goods-box .top .top-info image {
    display: block;
    width: 160rpx;
    height: 160rpx;
    border-radius: 10rpx;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right {
    flex: 1;
    padding-left: 28rpx;
    box-sizing: border-box;
    display: flex;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right .flex {
    width: 100%;
    margin: auto;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right .flex .title {
    font-size: 24rpx;
    line-height: 36rpx;
    font-weight: 500;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right .flex .attr {
    font-size: 24rpx;
    color: #7d7d7d;
    margin: 15rpx 0;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right .flex .price {
    display: flex;
    align-items: center;
  }
   .bottom .goods-warp .goods-box .top .top-info .top-right .flex .price text:last-of-type {
    color: white;
    font-size: 24rpx;
    color: #e20001;
  }
   .bottom .goods-warp .goods-box .bottoms {
    height: 70rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 28rpx;
  }
   .bottom .goods-warp .goods-box .bottoms .left {
    flex: 1;
    font-size: 24rpx;
    color: #1a1a1a;
  }
   .bottom .goods-warp .goods-box .bottoms .left .stepper {
    display: flex;
    justify-content: flex-end;
    height: 60rpx;
    box-sizing: border-box;
    align-items: center;
    font-size: 24rpx;
  }
   .bottom .cell-box {
    width: 100%;
    height: 80rpx;
    box-sizing: border-box;
    background: white;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 28rpx;
  }
   .bottom .cell-box .cell-left {
    color: #1a1a1a;
    font-size: 24rpx;
  }
   .bottom .cell-box .cell-right {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 24rpx;
    color: #666666;
  }
   .bottom .cell-box .cell-right i {
    font-size: 32rpx;
    color: #666666;
  }
   .bottom .cell-box .cell-right text {
    color: #666666;
  }
   .bottom .cell-box .cell-right .price {
    color: #e20001;
  }
   .footer {
    width: 100%;
    height: 98rpx;
    box-sizing: border-box;
    background: white;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    align-items: center;
    padding: 10rpx 28rpx;
  }
   .footer .left {
    display: flex;
    justify-content: flex-end;
    padding-right: 28rpx;
    flex: 1;
    font-size: 28rpx;
    color: #1a1a1a;
  }
   .footer .left text {
    color: #e20001;
  }
   .footer .right {
    width: 250rpx;
    font-size: 28rpx;
  }
   .footer .right .btn {
    background: #0200fb;
    color: white;
    border-radius: 40rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
  }
   
   .coupon-box {
    height: 80%;
    overflow: auto;
    box-sizing: border-box;
    background: #f7f7f7;
    display: flex;
    flex-direction: column;
    overflow: hidden;
  }
   .coupon-box .top {
    width: 100%;
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 28rpx;
    position: relative;
    background: white;
    border-bottom: 2rpx solid #f7f7f7;
    box-sizing: border-box;
  }
   .coupon-box .top i {
    font-size: 42rpx;
    color: #666666;
    position: absolute;
    right: 28rpx;
    top: 50%;
    transform: translateY(-50%);
  }
   .coupon-box .top .title {
    font-size: 28rpx;
    color: #666666;
  }
   .coupon-box .bottom {
    flex: 1;
    box-sizing: border-box;
    padding: 28rpx;
  }
   .coupon-box .bottom .loading-box {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    display: flex;
  }
   .coupon-box .bottom .loading-box .loading {
    margin: auto;
  }
   .coupon-box .bottom .list-warp {
    height: 290rpx;
    width: 100%;
    border-radius: 10rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    margin-bottom: 15rpx;
  }
   .coupon-box .bottom .list-warp .upper {
    flex: 1;
    display: flex;
    color: white;
    align-items: center;
    box-sizing: border-box;
  }
   .coupon-box .bottom .list-warp .upper .upper-box {
    display: flex;
    width: 100%;
    height: 100%;
    padding: 25rpx;
    box-sizing: border-box;
  }
