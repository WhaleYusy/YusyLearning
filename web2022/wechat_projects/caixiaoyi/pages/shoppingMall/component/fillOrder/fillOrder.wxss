    width: 230rpx;
    font-size: 29rpx;
    display: flex;
    align-items: center;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .left text {
    font-size: 63rpx;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .center {
    flex: 1;
    display: flex;
    align-items: center;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .center .msg {
    display: flex;
    flex-direction: column;
    font-size: 29rpx;
    padding-left: 10rpx;
    line-height: 44rpx;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .right {
    width: 160rpx;
    display: flex;
    align-items: center;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .right .btn {
    width: 100%;
    color: #fe874f;
    font-size: 29rpx;
    background: white;
    padding: 14rpx 20rpx;
    border-radius: 40rpx;
    text-align: center;
  }
   .coupon-box .bottom .list-warp .upper .upper-box .right .btn-color {
    color: #b5b5b5;
  }
   .coupon-box .bottom .list-warp .lower {
    flex: 1;
    display: flex;
    align-items: center;
  }
   .coupon-box .bottom .list-warp .lower .lower-box {
    box-sizing: border-box;
    padding: 25rpx;
  }
   .coupon-box .bottom .list-warp .lower .lower-box p {
    color: #6c6c6c;
    font-size: 25rpx;
    line-height: 46rpx;
  }
