<!--pages/purchase/component/xyAssistant/systemAssitant/systemAssitant.wxml-->
<!-- 人脉列表 -->
<van-collapse value="{{activeCodes}}" bind:change="onchangeCode">
  <block wx:for="{{rmList}}" wx:key="index">
      <van-collapse-item title="{{item.name}}" data-index="{{index}}" name="{{item.code}}" >              
        <block  wx:for="{{item.children}}"  wx:key="index" wx:for-item="itemCon">
            <view>{{itemCon.name}}</view>
        </block>                
        <view wx:if="{{item.children.length == 0}}" class='no-more' style='margin:0' >暂无数据</view>             
      </van-collapse-item>
  </block>
  <block>
    <van-collapse-item title="6. 我要加入社群">
      <image class="imgg" src="{{image1}}"></image>
    </van-collapse-item>
  </block>
  <block>
     <van-collapse-item title="7. 我要关注公众号">
      <image class="imgg" src="{{image2}}"></image>
    </van-collapse-item>
  </block>
</van-collapse>
  <view class="gotoSx" catchtap="gotoGf">
    <view>新圈友</view>
    <view class="rightBox"><text class="redCode" wx:if="{{unreadNum>0}}">{{unreadNum}}</text><van-icon name="arrow" size="32rpx" /></view>
    
  </view>
  <view class="gotoSx" catchtap="gotoSx">
    <view>系统消息</view>
    <view class="rightBox"><text class="redCode" wx:if="{{unreadNumMsg>0}}">{{unreadNumMsg}}</text><van-icon name="arrow" size="32rpx" /></view>
  </view>
