  .content_list_box {
    margin: 12rpx;
    background: #fff;
    display: flex;
    padding: 10rpx;
    box-sizing: border-box;
    box-shadow: 0 2rpx 12rpx 0 rgba(0, 0, 0, 0.1);
    border-radius: 10rpx;
  }
  .content_list_box .left-warp {
    flex-basis: 180rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
  }
  .content_list_box .left-warp .shop_img {
    width: 100%;
    height: 120rpx;
  }
  .content_list_box .left-warp .seop_img_h {
    height: 180rpx;
  }
  .content_list_box .left-warp .left-btn {
    flex: 1;
    font-size: 24rpx;
    display: flex;
    justify-content: space-around;
    box-sizing: border-box;
    margin-top: 15rpx;
  }
  .content_list_box .left-warp .left-btn text {
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    display: inline-block;
    border-radius: 10rpx;
    background: #d81e06;
    padding: 5rpx;
    color: #ffffff;
  }
  .content_list_box .left-warp .left-btn .postMsg {
    height: 40rpx;
    text-align: center;
    border: none;
    display: inline-block;
    border-radius: 10rpx;
    padding: 5rpx;
    color: #ffffff;
  }
  .content_list_box .left-warp .left-btn .span-active, .content_list_box .left-warp .left-btn .addF, .content_list_box .left-warp .left-btn .supplying {
    background: red;
  }
  .content_list_box .left-warp .left-btn .green {
    background: green;
  }
  .content_list_box .right_box {
    width: 73%;
    padding: 10rpx 0rpx 0 20rpx;
    box-sizing: border-box;
    position: relative;
    display: flex;
    line-height: 0.5rem;
  }
  .content_list_box .right_box .right-warp {
    width: 100%;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
  }
  .content_list_box .right_box .right-warp p {
    line-height: 40rpx !important;
  }
  .content_list_box .right_box .right-warp .isStick {
    color: white;
    position: absolute;
    right: 0;
    top: 50rpx;
    background: red;
    padding: 7rpx;
    border-radius: 5rpx;
  }
  .content_list_box .right_box .right-warp .right_box_p1 {
    font-size: 32rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 5rpx 0;
  }
  .content_list_box .right_box .right-warp .right_box_p2 {
    margin-top: 5rpx;
    font-size: 20rpx;
    color: red;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }
  .content_list_box .right_box .right-warp .right_box_p2 text {
    padding: 5rpx 30rpx 5rpx 0;
  }
  .content_list_box .right_box .right-warp .right_box_p3 {
    margin-top: 8rpx;
    font-size: 21rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }
  .content_list_box .right_box .right-warp .right_box_p4 {
    font-size: 21rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
  }
  
