/* pages/playAllegro/playThird/playThird.wxss */
.playThirdContent{
    margin-bottom:120rpx;
}
.link {
    color: black;
    background: lightgray !important;
  }
  
  .payPassword {
    z-index: 10000;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  .payPassword input {
    margin-top: -3rem;
    width: 7rem;
    height: 1.3rem;
    padding: 5rpx;
    box-sizing: border-box;
    margin-bottom: 0.5rem;
  }
  .payPassword .btnGroup {
    width: 7rem;
    display: flex;
    justify-content: space-between;
  }
  .payPassword .btnGroup .sure, .payPassword .btnGroup .cancel {
    width: 3rem;
    height: 1rem;
    background: red;
    color: white;
    font-size: 0.45rem;
    text-align: center;
    line-height: 1rem;
    cursor: pointer;
  }
  
  .second_step_box {
    width: 90%;
    background: #ffffff;
    padding: 10rpx 20rpx 0;
    box-sizing: border-box;
    margin: 20rpx auto;
    border-radius: 5rpx;
    box-shadow: 3rpx 3rpx 3rpx #e4e4e4;
  }
  .second_step_box .cont_title {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #5d89df;
    font-weight: bold;
    border-bottom: 1rpx solid #e0e0e0;
    display:flex;
    align-items: center;

  }
  .second_step_box .cont_title .text{
      display:flex;
      align-items: center;
  } 
  .second_step_box .cont_title text {
    color: red;
    margin-left: 15rpx;
  }
  .cont_content:first-of-type{
    width:180rpx;
  }
  .second_step_box .cont_content {
    width: 100%;
    height: 150rpx;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
    color: #3a3a3a;
    border-bottom: 1rpx solid #c5c5c5;
    position: relative;
    padding-top: 50rpx;
    box-sizing: border-box;
  }
  .second_step_box .cont_content .tips {
    position: absolute;
    font-size: 25rpx;
    bottom: 30rpx;
    right: 0rpx;
  }
  .second_step_box .cont_content_o {
    margin-top: 25rpx;
    justify-content: flex-start;
  }
  .second_step_box .cont_content_o text {
    display: inline-block;
    width: 23%;
  }
  
  .codeBox {
