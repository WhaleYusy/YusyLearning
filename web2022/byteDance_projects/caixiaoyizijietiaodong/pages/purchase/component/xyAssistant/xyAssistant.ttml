      </view>
      <view>
       <block wx:for="{{rmList}}" tt:for-item="item" tt:for-index="idx">
          <!-- 订单组list -->
          <view style='margin-bottom:10rpx;' data-index="{{idx}}">
            <view class="li" data-index="{{idx}}" bindtap='changeToggle'>
              <view class="left">
                <view>{{item.name}}（{{item.sum}}）</view>
              </view>
              <view class="right">
                <image class="select_img1 {{!selectedFlag[idx]?' ':'select_img_rotate1'}}" src='http://qiniuoss.822644.com/wxPic/20191122/765261961435844608.png' style="margin-top:20rpx"></image>   
              </view>
            </view>
            <view tt:if="{{item.children.length>0}}" hidden="{{!selectedFlag[idx]}}">
              <view class="b-shadow">
               <block  tt:for="{{item.children}}"  tt:key="index" tt:for-item="itemCon">
                <view class="xya-rmli" data-item="{{itemCon}}"  catchtap="goChatDetail2" >
                  <view class="xya-rmliImg">
                  <image src="{{itemCon.headurl}}"></image>
                  </view>
                  <view class="xya-rmliCon">
                    <view class="xya-rmliTop">
                      <view class="xya-rmlivipicon">
                        <!-- <van-icon tt:if="{{itemCon.userLevelId != 1}}" name="star" size="22rpx" color="#CD7F32"></van-icon>
                        <van-icon wx:else name="star" size="22rpx"></van-icon> -->
                          <i class="iconfont iconxingxing2"  tt:if="{{itemCon.userLevelId != 1}}" style="color:#CD7F32"></i>
                          <i class="iconfont iconxingxing2"  tt:else></i>
                        <text style="color:#CD7F32;" tt:if="{{itemCon.userLevelId != 1}}">{{itemCon.levelName}}</text>
                        <text wx:else >{{itemCon.levelName}}</text>
                      </view>
                      <view class="xya-rmliName"> {{itemCon.nickname}}</view>
                      <view>粉丝：{{itemCon.fansCount}}</view>
                    </view>
                    <view class="xya-rmliBottom">供应产品：{{itemCon.operateProduces}}</view>
                  </view>
                </view>
              </block> 
              </view>
            </view>
            <view class="no-more" style='margin:8px' hidden="{{!selectedFlag[idx]}}" tt:if="{{rmListMore[idx]}}" data-index="{{idx}}" catchtap="loadMoreNew">加载更多</view>
            <view tt:elif="{{item.children.length != 0 && !rmListMore[idx]}}" hidden="{{!selectedFlag[idx]}}" class='no-more' style='margin:8px' >暂无更多</view>  
            <view tt:if="{{item.children.length == 0}}" hidden="{{!selectedFlag[idx]}}" class='no-more' style='margin:8px' >暂无数据</view>  
           </view>
          <!-- end订单组1 -->
         </block> 
       </view>
