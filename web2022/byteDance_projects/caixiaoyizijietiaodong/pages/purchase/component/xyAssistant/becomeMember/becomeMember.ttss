  background: #fff;
  text-align: center;
  font-size:30rpx;
  border-radius: 8rpx; }
.update_cont .update_cont_line .active {
  background: #ffdd26;
  border: 2rpx solid #ffdd26;
  color: red; }
 .update_cont .update_cont_line .custmer {
  font-size: 35rpx; }
