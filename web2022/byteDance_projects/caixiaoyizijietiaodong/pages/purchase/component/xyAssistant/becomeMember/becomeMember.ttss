  padding: 20rpx 50rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #cccccc; }
.update_cont .update_cont_line {
  display: flex;
  font-size: 30rpx;
  margin-top: 30rpx; }
.update_cont .update_cont_line .vip {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
