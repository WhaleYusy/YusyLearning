}
/* 样式 */

 .title_line {
  width: 100%;
  font-size: 30rpx;
  padding: 15rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  border-bottom: 1rpx solid #cccccc; }
.title_line text {
  margin-top: 20rpx;
  margin-left: 10rpx; }
.update_cont {
  width: 100%;
